{
    "name": "elife/api-sdk",
    "description": "eLife Sciences API SDK",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "eLife\\ApiSdk\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\eLife\\ApiSdk\\": "./test"
        }
    },
