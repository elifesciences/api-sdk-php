{
    "name": "elife/api-sdk",
    "description": "eLife Sciences API SDK",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "eLife\\ApiSdk\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Csa\\Bundle\\GuzzleBundle\\": "./vendor-extra/Csa/GuzzleBundle",
            "test\\eLife\\ApiSdk\\": "./test"
        }
    },
    "require": {
        "php": "^7.0",
        "elife/api-client": "^1.0@dev",
        "guzzlehttp/promises": "^1.0",
        "ocramius/package-versions": "^1.1",
        "symfony/serializer": "^3.1"
    },
    "require-dev": {
        "elife/api": "dev-master",
        "elife/api-validator": "^1.0@dev",
        "guzzlehttp/guzzle": "^6.0",
        "phpunit/phpunit": "^5.2",
        "psr/http-message": "^1.0",
        "puli/composer-plugin": "^1.0-beta9",
        "puli/repository": "^1.0-beta9",
        "webmozart/json": "^1.3@dev"
    },
    "minimum-stability": "beta",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
