        "elife/api-client": "^1.0@dev",
        "guzzlehttp/promises": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
